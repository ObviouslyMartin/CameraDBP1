<!DOCTYPE html>
<html>
<head>
    <title>Camera Body - View all </title>
</head>
<body>
<h1>Cameras</h1>
<a href="/Camera_Body/add">Add New Camera Body</a> <br />
<table border="1">
    <tr>
        <th>ID</th>
        <th>Manufacturer</th>
        <th>Name</th>
        <th>Still Image Resolution</th>
        <th>Sensor Type</th>
        <th>Lens Mount</th>
        <th>Auto Focus Points</th>
        <th>Actions</th>
    </tr>
    <%for(var i=0; i< cameras.length; i++ ) { %>
    <tr>
        <td><%= cameras[i].Camera_Body_Id%></td>
        <td><%= cameras[i].Camera_Manufacturer%></td>
        <td><%= cameras[i].Name%></td>
        <td><%= cameras[i].Still_Image_Resolution%></td>
        <td><%= cameras[i].Sensor_Type%></td>
        <td><%= cameras[i].Lens_Mount%></td>
        <td><%= cameras[i].Auto_Focus_Points%></td>
        <td>
            <a href ='/Camera_Body/edit?Camera_Body_Id=<%= cameras[i].Camera_Body_Id%> '>
                Edit
            </a>
        </td>
    </tr>
    <%}%>
</table>
</body>
</html>